#include<iostream>
#include<cmath>

using namespace std;

int main(int argc, char ** argv) {
  int test_case = 0;
  int T = 0;
  int num = 0;
  cin >> T;
  for (test_case = 1; test_case <= T; ++test_case) {
    double total = 0;
    for (int i = 1; i <= 10; i++) {
      cin >> num;
      total += num;
    }
    cout << "#" << test_case << " " << round(total/10) << endl;
  }
  return 0;
}